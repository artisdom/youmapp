/**app.wxss**/
@import "ionicons.wxss";
/* reset css*/
blockquote, body, button, caption, dd, div, dl, dt, fieldset, figure, form, h1,
h2, h3, h4, h5, h6, hr, html, input, legend, li, menu, ol, p, pre, table, td,
textarea, th, ul {
    margin: 0;
    padding: 0;
    outline: 0;
}

address, article, aside, details, figcaption, figure, footer, header, hgroup,
menu, nav, section {
    display: block;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

caption, th {
    text-align: left;
    font-weight: 400;
}

abbr, body, fieldset, html, iframe, image {
    border: 0;
}

address, cite, dfn, em, i, var {
    font-style: normal;
}

[hidefocus], summary {
    outline: 0;
}

li {
    list-style: none;
}

h1, h2, h3, h4, h5, h6, small {
    font-size: 100%;
}

sub, sup {
    font-size: 83%;
}

code, kbd, pre, samp {
    font-family: inherit;
}

q:after, q:before {
    content: none;
}

textarea {
    overflow: auto;
    resize: none;
}

label, summary {
    cursor: default;
}

a, button {
    cursor: pointer;
}

b, em, h1, h2, h3, h4, h5, h6, strong {
    font-weight: 400;
}

a, a:hover, del, ins, s, u {
    text-decoration: none;
    outline: 0;
}

body, button, input, keygen, legend, select, textarea {
    font-size: 56rpx;
    line-height: 1.5;
    color: #333;
    outline: 0;
    font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

input::-ms-clear, input::-ms-reveal {
    display: none;
}

html {
    -ms-touch-action: manipulation;
    touch-action: manipulation;
}

input {
    box-shadow: none;
}

video {
    width: 100%;
    height: auto;
}

::-webkit-input-placeholder {
    color: #999;
}

:-moz-placeholder {
    color: #999;
}

::-moz-placeholder {
    color: #999;
}

:-ms-input-placeholder {
    color: #999;
}

